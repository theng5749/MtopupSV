
<div class="row">
    @if (ViewBag.ResultCode == "200")
    {
        <div class="panel panel-primary">
            <div class="panel-body">
                <img src="~/Files/iconcomfirm.png" height="80" width="90" style="margin-left:auto;margin-right:auto;display:block;margin-bottom:16px;" />
                <h4 style="text-align: center;margin-bottom:16px;"><strong style="color:forestgreen;">@ViewBag.ResultDesc</strong></h4>
                <h5 style="text-align: center;">@ViewBag.Result</h5>
            </div>
        </div>
    }
    else
    {
        <div class="panel panel-danger">
            <div class="panel-body">
                <img src="~/Files/iconcomfirm.png" height="80" width="90" style="margin-left:auto;margin-right:auto;display:block;margin-bottom:16px;" />
                <h4 style="text-align: center;margin-bottom:16px;"><strong style="color:red;">@ViewBag.ResultDesc</strong></h4>
                <h5 style="text-align: center;">@ViewBag.Result</h5>
            </div>
        </div>
    }
</div>



